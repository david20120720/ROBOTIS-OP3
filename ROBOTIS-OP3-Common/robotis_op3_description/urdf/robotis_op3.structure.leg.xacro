<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  
  <xacro:property name="pi" value="3.141592654" />

  <!--//////////////////////////////////////////////////////////////////////////////////////-->
  <!--//////////////////////////////// robotis_op3 right leg ////////////////////////////////-->
  <!--//////////////////////////////////////////////////////////////////////////////////////-->   
  <!-- robotis_op3 right leg -->
  <xacro:macro name="robotis_op3_r_leg" params="parent">
  
  <!-- right hip yaw joint -->
  <joint name="robotis_op3_r_leg_hip_y" type="revolute">
    <axis xyz="0 0 -1" />    
    <origin xyz="0.0 -0.035 -0.036" rpy="0 0 0"/>
    <parent link="${parent}"/>
    <child  link="robotis_op3_r_leg_hip_y_link"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint> 
  <xacro:insert_transmission name="robotis_op3_r_leg_hip_y" />    

  <!-- right hip yaw link -->
  <link name="robotis_op3_r_leg_hip_y_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robotis_op3_description/meshes/robotis_op3_r_leg_hip_y_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>

  <!-- right hip roll joint -->
  <joint name="robotis_op3_r_leg_hip_r" type="revolute">
    <axis xyz="-1 0 0" />    
    <origin xyz="-0.021 0.0 -0.0285" rpy="0 0 0"/>
    <parent link="robotis_op3_r_leg_hip_y_link"/>
    <child  link="robotis_op3_r_leg_hip_r_link"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint> 
  <xacro:insert_transmission name="robotis_op3_r_leg_hip_r" />    

  <!-- right hip roll link -->
  <link name="robotis_op3_r_leg_hip_r_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robotis_op3_description/meshes/robotis_op3_r_leg_hip_r_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  
  <!-- right hip pitch joint -->
  <joint name="robotis_op3_r_leg_hip_p" type="revolute">
    <axis xyz="0 1 0" />    
    <origin xyz="0.0211 0.019 0.0" rpy="0 0 0"/>
    <parent link="robotis_op3_r_leg_hip_r_link"/>
    <child  link="robotis_op3_r_leg_hip_p_link"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint> 
  <xacro:insert_transmission name="robotis_op3_r_leg_hip_r" />    

  <!-- right hip pitch link -->
  <link name="robotis_op3_r_leg_hip_p_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robotis_op3_description/meshes/robotis_op3_r_leg_hip_p_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>

  <!-- right knee pitch joint -->
  <joint name="robotis_op3_r_leg_kn_p" type="revolute">
    <axis xyz="0 1 0" />    
    <origin xyz="0.0 0.0 -0.11" rpy="0 0 0"/>
    <parent link="robotis_op3_r_leg_hip_p_link"/>
    <child  link="robotis_op3_r_leg_kn_p_link"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint> 
  <xacro:insert_transmission name="robotis_op3_r_leg_kn_p" />    

  <!-- right knee pitch link -->
  <link name="robotis_op3_r_leg_kn_p_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robotis_op3_description/meshes/robotis_op3_r_leg_kn_p_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  
  <!-- right ankle pitch joint -->
  <joint name="robotis_op3_r_leg_an_p" type="revolute">
    <axis xyz="0 -1 0" />    
    <origin xyz="0.0 0.0 -0.11" rpy="0 0 0"/>
    <parent link="robotis_op3_r_leg_kn_p_link"/>
    <child  link="robotis_op3_r_leg_an_p_link"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint> 
  <xacro:insert_transmission name="robotis_op3_r_leg_an_p" />    

  <!-- right ankle pitch link -->
  <link name="robotis_op3_r_leg_an_p_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robotis_op3_description/meshes/robotis_op3_r_leg_an_p_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
  
  <!-- right ankle roll joint -->
  <joint name="robotis_op3_r_leg_an_r" type="revolute">
    <axis xyz="1 0 0" />    
    <origin xyz="-0.021 -0.019 0.0" rpy="0 0 0"/>
    <parent link="robotis_op3_r_leg_an_p_link"/>
    <child  link="robotis_op3_r_leg_an_r_link"/>
    <limit velocity="6.5" effort="1000" lower="${-pi*0.5}" upper="${pi*0.5}" />
    <dynamics damping="0.1" friction="0.0" />
  </joint> 
  <xacro:insert_transmission name="robotis_op3_r_leg_an_r" />    

  <!-- right ankle roll link -->
  <link name="robotis_op3_r_leg_an_r_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://robotis_op3_description/meshes/robotis_op3_r_leg_an_r_link.stl" scale="0.001 0.001 0.001"/>
      </geometry>
      <material name="grey"/>
    </visual>
  </link>
    
  </xacro:macro>
    
    

